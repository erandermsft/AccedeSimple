{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
      "principalId": {
        "value": "${AZURE_PRINCIPAL_ID}"
      },
      "AzureAIFoundryProject": {
        "value": "${AZURE_AZURE_AIFOUNDRY_PROJECT}"
      },
      "AzureOpenAIEndpoint": {
        "value": "${AZURE_AZURE_OPEN_AIENDPOINT}"
      },
      "AzureOpenAIResourceGroup": {
        "value": "${AZURE_AZURE_OPEN_AIRESOURCE_GROUP}"
      },
      "AzureOpenAIResourceName": {
        "value": "${AZURE_AZURE_OPEN_AIRESOURCE_NAME}"
      },
      "AzureResourceGroup": {
        "value": "${AZURE_AZURE_RESOURCE_GROUP}"
      },
      "AzureSubscriptionId": {
        "value": "${AZURE_AZURE_SUBSCRIPTION_ID}"
      },
      "environmentName": {
        "value": "${AZURE_ENV_NAME}"
      },
      "location": {
        "value": "${AZURE_LOCATION}"
      }
    }
  }
  